@model ICollection<ToDoWebApp.Models.Todo>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>List</title>
</head>
<body>
    <h1>ToDos for today:</h1>
    <ul>
        @{

            foreach (var item in @Model)
            {
                <li>@item.Title</li>
            }

        }
    </ul>

</body>
</html>